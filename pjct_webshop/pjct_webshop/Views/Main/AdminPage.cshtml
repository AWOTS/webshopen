@{
    Layout = "~/Views/Shared/_LayoutPage.cshtml";
}

<div class="container">
    <div class="row">
        <div class="col-md-3"></div>

        <div class="box col-md-6">
            <h6>Produktnamn<label class="alert-warning">*</label></h6>
            <input type="text" class="form-control" placeholder="Namn">
            <h6>Beskrivning<label class="alert-warning">*</label></h6>
            <input type="text" class="form-control" placeholder="Beskrivning">
            <h6>Bildkälla<label class="alert-warning">*</label></h6>
            <input type="text" class="form-control" placeholder="Bildens adress">
            <h6>Antal<label class="alert-warning">*</label></h6>
            <input type="number" class="form-control" placeholder="antal som finns tillgängliga">
            <h6>Pris<label class="alert-warning">*</label></h6>
            <input type="number" class="form-control" placeholder="Pris i kr">
            <h3>Tillgänglig efter produkt sålt slut?</h3>
            <label class="radio-inline"><input type="radio" name="optradio">Ja</label>
            <label class="radio-inline"><input type="radio" name="optradio">Nej</label>
        </div>
        <div class="col-md-3"></div>
    </div>
    <div class="row">
        <div class="col-md-3"></div>

        <div class="box col-md-6">

        </div>
        <div class="col-md-3"></div>
    </div>
</div>